<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <ApiVersion>1.2</ApiVersion>
    <VSVer>12</VSVer>
    <GlibEtcInstallRoot>$(SolutionDir)\..\..\..\..\..\..\gtk\$(Platform)</GlibEtcInstallRoot>
    <CopyDir>..\..\..\..\Json-glib-rel</CopyDir>
    <DefDir>$(SolutionDir)$(Configuration)\$(PlatformName)\obj\$(ProjectName)\</DefDir>
    <JsonGlibLibtoolCompatibleDllPrefix>lib</JsonGlibLibtoolCompatibleDllPrefix>
    <JsonGlibLibtoolCompatibleDllSuffix>-$(ApiVersion)</JsonGlibLibtoolCompatibleDllSuffix>
    <JsonGlibSeparateVSDllPrefix />
    <JsonGlibSeparateVSDllSuffix>-$(ApiVersion)</JsonGlibSeparateVSDllSuffix>
    <JsonGlibDllPrefix>$(JsonGlibSeparateVSDllPrefix)</JsonGlibDllPrefix>
    <JsonGlibDllSuffix>$(JsonGlibSeparateVSDllSuffix)</JsonGlibDllSuffix>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>JSonversionpathsprops</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="ApiVersion">
      <Value>$(ApiVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="GLibEtcInstallRoot">
      <Value>$(GLibEtcInstallRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="CopyDir">
      <Value>$(CopyDir)</Value>
    </BuildMacro>
    <BuildMacro Include="DefDir">
      <Value>$(DefDir)</Value>
    </BuildMacro>
    <BuildMacro Include="JSonLibtoolCompatibleDllPrefix">
      <Value>$(JSonLibtoolCompatibleDllPrefix)</Value>
    </BuildMacro>
    <BuildMacro Include="JSonLibtoolCompatibleDllSuffix">
      <Value>$(JSonLibtoolCompatibleDllSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="JSonSeparateVSDllPrefix">
      <Value>$(JSonSeparateVSDllPrefix)</Value>
    </BuildMacro>
    <BuildMacro Include="JSonSeparateVSDllSuffix">
      <Value>$(JSonSeparateVSDllSuffix)</Value>
    </BuildMacro>
    <BuildMacro Include="JSonDllPrefix">
      <Value>$(JSonDllPrefix)</Value>
    </BuildMacro>
    <BuildMacro Include="JSonDllSuffix">
      <Value>$(JSonDllSuffix)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
